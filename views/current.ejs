<% include header.ejs %>
<% include nav.ejs %>
<body class="content brightness">
<div id="page-wrapper">
    <div class="container-fluid">
        <div class="col-xs-12 col-sm-12 col-md-10 col-md-offset-1" ng-controller="CurrentController">
            <h1><span
                        class="glyphicon glyphicon-play-circle"></span> {{ currentSong.Title }}</h1>
            <br>
            <b>{{ currentSong.Album }}</b>
            <br>
            <span>{{currentSong.Artist}}</span>
            <br>
            <br><i>{{ currentSong.file }}</i>
        </div> <!-- /container -->
        <div class="col-xs-12 col-sm-12 col-md-10 col-md-offset-1" ng-controller="SeekerController">
            <div class="seekBase" seek-track>
                <div class="seekLoad" ng-style="{width : ( seekPercent + '%' ) }"></div>
            </div>
        </div>
    </div>
</div>
</body>

</html>