<% include header.ejs %>
<% include nav.ejs %>
<body class="content brightness">
<div id="page-wrapper">

    <div class="container-fluid">
        <div class="col-xs-12 col-sm-12 col-md-10 col-md-offset-1">
            <h1><span
                        class="glyphicon glyphicon-stats
                        "></span> MPD Status</h1>
            <br>
            <table class="table table-responsive">
                <% var keysbyindex = Object.keys(status);
                for (var i=0; i<keysbyindex.length; i++){ %>
                <tr>
                    <td><%= keysbyindex[i].toUpperCase()%></td>
                    <td><%= status[keysbyindex[i]] %></td>
                </tr>
                <% }%>
            </table>
        </div> <!-- /container -->
    </div>
</div>
</body>

</html>